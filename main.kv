#:import NoTransition kivy.uix.screenmanager.NoTransition
<RootScreen>:
    StartScreen:
        name:'main'
    About_App:
        name:'about_app'
    Lab:
        name:'laboratory'
    Forgot_password:
        name:"forgot_password"
    Login_user:
        name:'login_user'
    Signup_user:
        name:'signup_user'
    Login_admin:
        name:'login_admin'
    Show_lab:
        name:'show_lab'
    Signup_admin:
        name:'signup_admin'
    Profile:
        name:"profile"
    
    WhoAreYou:
        name:"who"
    Choose_entery:
        name:'choose_entry'
    Choose_signup:
        name:'choose_signup'
    Show_Analysis:
        name:'show_analysis'
    Update:
        name:'update'
    Forgot_password_admin:
        name:'forget_admin'

<StartScreen>:
    MDFloatLayout:
        md_bg_color:1,1,1,1
        Image:
            source:"img/logo.png"
            size_hint: .8,.8
            pos_hint: {'center_x':.5 , 'center_y':.65 }
        
        MDLabel:
            text:"Hello !"
            font_name: 'BPoppins'
            font_size: "23sp"
            pos_hint:{"center_y":.38}
            halign: 'center'
            color: rgba(34,34,34,255)
            
        MDLabel:
            text:"This application is an analysis laboratory that makes it easy for the user to find the nearest laboratory to him"
            font_name: 'MPoppins'
            font_size: "14sp"
            size_hint_x:.75
            pos_hint:{"center_x":.5 ,"center_y":.3}
            halign: 'center'
            color: rgba(127,127,127,255)

        MDRaisedButton:
            text:'LOGIN'
            size_hint:.66,.06
            pos_hint:{"center_x":.5,"center_y":.18}
            md_bg_color: "blue"
            
            font_name:'BPoppins'
            on_release: 
                root.manager.transition.direction="left"
                root.manager.current="choose_entry"
            canvas.before:
                RoundedRectangle:
                    size:self.size
                    pos:self.pos
                    radius: [5]


        MDRaisedButton:
            text:'SIGNUP'
            size_hint:.66,.06
            pos_hint:{"center_x":.5,"center_y":.09}
            md_bg_color: "white"
            font_name:'BPoppins'
            theme_text_color: "Custom"
            text_color: "blue"
            on_release: 
                root.manager.transition.direction="left"
                root.manager.current="choose_signup"

            canvas.before:
                Color:
                    rgb: rgba(52,0,231,255)
                Line:
                    width: 1.2
                    rounded_rectangle: self.x ,self.y ,self.width ,self.height , 5, 5, 5, 5, 100

